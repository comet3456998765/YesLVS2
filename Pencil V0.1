			local x1, y1, z1 = p0:ToOrientation()
			local cf = CFrame.new(p0.p, p1.p)
			local x2, y2, z2 = cf:ToOrientation()
			local d = math.deg
			if deg then
				return Vector3.new(d(x1-x2), d(y1-y2), d(z1-z2))
			else
				return Vector3.new((x1-x2), (y1-y2), (z1-z2))
			end
		end


